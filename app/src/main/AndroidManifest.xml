<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zqx.pwd">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name="com.zqx.pwd.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zqx.pwd.ui.activity.CheckInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zqx.pwd.ui.activity.AccountsActivity" />
        <activity android:name="com.zqx.pwd.ui.activity.SettingsActivity" />
    </application>

</manifest>